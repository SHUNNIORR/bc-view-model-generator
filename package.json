{
  "name": "bc-view-model-generator",
  "displayName": "bc-view-model-generator",
  "publisher": "Shunnior",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
        "commands": [
            {
                "command": "bc-view-model-generator.createViewModel",
                "title": "New view-model"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "bc-view-model-generator.createViewModel",
                    "when": "explorerResourceIsFolder",
                    "group": "navigation"
                }
            ]
        }
    },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
